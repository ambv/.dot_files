# Cygwin specific settings
export DISPLAY=":0"
export LS_OPTIONS="$LS_OPTIONS -G"
export EDITOR=$HOME'/.dot_files/bin/cyg-wrapper.sh "/cygdrive/c/Tools/Vim/vim73/gvim.exe" --cyg-env-clear=HOME --binary-opt=-c,--cmd,-T,-t,--servername,--remote-send,--remote-expr --fork=1'
alias ls="ls $LS_OPTIONS"
alias gvim="$EDITOR"
alias vim="$EDITOR"
alias vi="$EDITOR"
