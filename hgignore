# Compiled Python scripts
syntax: glob

*.pyc
*.pyo

# Compiled Java classes
syntax: glob

*.class

# SQLite databases
syntax: glob

*.db
*.sqlite

# Temporary or runtime files
syntax: glob

*.old
*.bak
*.swp
*.local
*.log
*.pid
*.cache
*.orig
*~

# Python Sphinx and setuptools files
syntax: regexp

^doc/_build
^build
^dist

syntax: glob

*.egg-info

# Mac OS X specific
syntax: glob

.DS_Store
