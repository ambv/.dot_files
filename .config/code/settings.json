{
    "python.pythonPath": "/Users/ambv/.virtualenvs/notes/bin/python",
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": false,
    "python.linting.enabled": true,
    "python.formatting.provider": "black",
    "python.analysis.typeCheckingMode": "basic",
    "editor.fontFamily": "TerminusTTF",
    "workbench.iconTheme": "file-icons-colourless",
    "editor.quickSuggestions": {
        "other": false,
        "comments": false,
        "strings": false
    },
    "editor.find.autoFindInSelection": "multiline",
    "[python]": {
        "editor.rulers": [
            88
        ],
        "editor.formatOnType": true,
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "[restructuredtext]": {
        "editor.rulers": [72]
    },
    "[markdown]": {
        "editor.rulers": [72]
    },
    "[lua]": {
        "editor.tabSize": 2
    },
    "git.autofetch": true,
    "mypy.runUsingActiveInterpreter": true,
    "python.linting.flake8CategorySeverity.E": "Warning",
    "python.linting.flake8CategorySeverity.W": "Hint",
    "python.testing.unittestEnabled": true,
    "explorer.confirmDragAndDrop": false,
    "workbench.editor.showTabs": "multiple",
    "editor.minimap.renderCharacters": false,
    "editor.minimap.enabled": false,
    "editor.tabCompletion": "onlySnippets",
    "editor.autoClosingBrackets": "never",
    "git.confirmSync": false,
    "python.venvFolders": [
        "~/.venvs"
    ],
    "vim.cursorStylePerMode.normal": "block",
    "vim.cursorStylePerMode.insert": "line",
    "vim.cursorStylePerMode.replace": "block-outline",
    "vim.cursorStylePerMode.visual": "block-outline",
    "vim.cursorStylePerMode.visualblock": "block",
    "vim.cursorStylePerMode.visualline": "underline",
    "vim.enableNeovim": true,
    "vim.gdefault": true,
    "vim.ignorecase": false,
    "vim.hlsearch": true,
    "vim.useSystemClipboard": true,
    "vim.highlightedyank.enable": true,
    "vim.sneak": true,
    "vim.normalModeKeyBindings": [
        {
            "before": ["Y"],
            "after": ["y$"]
        }
    ],
    "workbench.preferredDarkColorTheme": "Chromodynamics Delek",
    "files.associations": {
        "*.json": "jinja-json"
    },
    "editor.autoClosingQuotes": "never",
    "editor.renderWhitespace": "none",
    "cSpell.userWords": [
